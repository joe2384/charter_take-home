{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "typeRoots": ["node_modules/@types", "src/types"],
    "suppressImplicitAnyIndexErrors": true,
  },
  "include": [
    "src", "src/store/actions/action.ts", "src/store/reducers/index.ts", "src/tests/Api.test.tsx", "src/tests/setupTests.ts"
  ]
}
